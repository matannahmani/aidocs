{
  "name": "aidocs",
  "version": "0.0.0",
  "description": "AI Docs is a tool for generating documentation from your code using docusaurus.",
  "type": "module",
  "bin": "build/src/main.js",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "devDependencies": {
    "@clack/prompts": "^0.6.3",
    "@types/jest": "~29.2",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~5.44",
    "@typescript-eslint/parser": "~5.44",
    "eslint": "~8.28",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "jest": "~29.3",
    "kolorist": "^1.7.0",
    "openai": "^3.2.1",
    "prettier": "~2.8",
    "rimraf": "~3.0",
    "ts-jest": "~29.0",
    "tsutils": "~3.21",
    "typescript": "~4.9",
    "zod": "^3.21.4"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "author": "Matan Nahmani",
  "license": "Apache-2.0",
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.2",
    "cleye": "^1.3.2",
    "tslib": "~2.4"
  },
  "volta": {
    "node": "18.12.1"
  }
}
